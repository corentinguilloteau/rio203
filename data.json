{
    <deviceid>: {
        type: <telemetry|power>,
        sensors: {
            <sensorid1>: {
                value: <value>
            },
            <sensorid2>: {
                value: <value>
            },
            ...
        }
    },
    ...
}

{
    "601336d78f2aee6dd20e92f4": {
        "type": "power",
        "sensors": {
            "0": {
